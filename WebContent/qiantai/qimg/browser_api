	        o.r = Math.floor(Math.random() * 2147483647);
	        //从a到z顺序生成参数
	        var r = [];
	        for (var x in o) {
	            if (typeof o[x] != 'function')
	                r.push(x + '=' + encodeURIComponent(o[x]));
	        }
         	var a = new Image(1, 1);
	        a.src = 'http://link.ipanshi.com/browser_data?' + r.join('&');
	        a.onload = function () {
	            a.onload = null;
	        };
